<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="reporte" namespace="/administrador/reporte" extends="default">


        <action name="reporteLoad" class="ReporteAction" method="cargarReporte">
            <result name="success" type="tiles">cargarReporte</result>
        </action>

        <action name="generarReporte" class="ReporteAction" method="generarReporte">
            <result name="success" type="tiles">cargarReporte</result>
        </action>



    </package>



    <package name="todosContacto" namespace="/todos/contacto" extends="default">
        <action name="cargarVerContacto" class="ContactoAction" method="cargarContacto">
            <result name="success" type="tiles">contactoPrinicipal</result>
        </action>
    </package>

</struts>
